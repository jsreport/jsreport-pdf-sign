{
  "name": "jsreport-pdf-sign",
  "version": "0.2.0",
  "description": "jsreport extension adding pdf signing support",
  "main": "index.js",
  "keywords": [
    "jsreport",
    "pdf"
  ],
  "homepage": "https://github.com/jsreport/jsreport-pdf-sign",
  "repository": {
    "type": "git",
    "url": "git@github.com:jsreport/jsreport-pdf-sign.git"
  },
  "author": {
    "name": "Jan Blaha",
    "email": "jan.blaha@hotmail.com"
  },
  "scripts": {
    "start": "set extensions_pdfSign_secret=ExchangePasswordPasswordExchange&&jsreport-studio-start --httpPort 3000",
    "test": "mocha test --timeout 8000 && standard",
    "build": "jsreport-studio-build",
    "prepublishOnly": "jsreport-studio-build"
  },
  "standard": {
    "ignore": [
      "main.js"
    ],
    "parser": "babel-eslint",
    "env": {
      "node": true,
      "mocha": true,
      "browser": true
    }
  },
  "license": "MIT",
  "dependencies": {
    "jsreport-pdfjs": "0.4.2",
    "node-signpdf": "0.3.0"
  },
  "devDependencies": {
    "babel-eslint": "10.0.1",
    "eslint": "5.16.0",
    "eslint-config-standard-jsx": "6.0.2",
    "eslint-plugin-babel": "5.3.0",
    "jsreport-assets": "1.1.3",
    "jsreport-chrome-pdf": "1.3.0",
    "jsreport-core": "2.5.0",
    "jsreport-studio-dev": "1.4.0",
    "jsreport-templates": "2.2.0",
    "mocha": "6.1.4",
    "puppeteer": "1.16.0",
    "should": "13.2.3",
    "standard": "12.0.1"
  },
  "files": [
    "lib",
    "studio",
    "index.js",
    "jsreport.config.js"
  ],
  "engines": {
    "node": ">=8.9"
  }
}
